@model  TeamJerassicpark.Models.EmpDept
@{
    ViewBag.Title = "GetMeView8";
}

<h2>GetMeView8</h2>

<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model.emp)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>
<br />

@Model.Dept.DeptId
@Model.Dept.DeptName
