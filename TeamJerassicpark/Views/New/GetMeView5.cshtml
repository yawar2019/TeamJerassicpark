
@{
    ViewBag.Title = "GetMeView5";
}

<h2>GetMeView5</h2>



<table class="table-bordered table-condensed table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>

    @foreach (var item in ViewBag.info)
    {
        <tr>
            <td>
                @item.EmpId
            </td>
            <td>
                @item.EmpName
            </td>
            <td>
                @item.EmpSalary
            </td>
        </tr>

    }

</table>

